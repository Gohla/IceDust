module ternary

  entity Supplier {
    name : String 1
  }

  entity Part {
    name : String 1
    nameMany : String 1 = name + "s" (default) // not all names are +s for multiple
  }

  entity Project {
    name : String 1
  }

  relation Supply {
    Supplier.supply * -> supplier 
    Part.supply * -> part
    Project.supply *  -> project
    amount : Int 1  
    
    name : String 1 = supplier.name + " supplies " + amount as String + " " + part.nameMany + " to " + project.name
  }
