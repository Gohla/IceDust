module icedust2

imports
  
  Common
  Expressions
  Types

context-free start-symbols
  
  Start

context-free syntax
  
  Start.Module = <
    module <ID>
    
    <{Definition "\n\n"}*>
      
  >

  Definition.Entity = <
    entity <ID> {
      <{Attribute "\n"}*>
    }
  >
  
  Definition.Relation = [
    relation [ID].[ID] [Multiplicity] <-> [Multiplicity] [ID].[ID]
  ]
  
  Attribute.Attribute = <
    <ID> : <Type> <Multiplicity>
  >
  
  Attribute.Attribute = <
    <ID> : <Type> <Multiplicity> = <Exp>
  >
