module analysis/expressions-multiplicity-operators

imports
 
  static-semantics

signature constructors
  
  ChoiceLeftMult : Type * Type -> TypeOp

rules

  nabl2-is-op-hook = ?ChoiceLeftMult(_,_)
  
  nabl2-reduce-op-hook(|sol): ChoiceLeftMult(TOne()       , _            ) -> TOne()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMult(TOneOrMore() , _            ) -> TOneOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMult(TZeroOrOne() , a            ) -> a
  nabl2-reduce-op-hook(|sol): ChoiceLeftMult(TZeroOrMore(), TOne()       ) -> TOneOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMult(TZeroOrMore(), TZeroOrOne() ) -> TZeroOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMult(TZeroOrMore(), TOneOrMore() ) -> TOneOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMult(TZeroOrMore(), TZeroOrMore()) -> TZeroOrMore()

signature constructors
  
  ChoiceLeftMerge : Type * Type -> TypeOp

rules

  nabl2-is-op-hook = ?ChoiceLeftMerge(_,_)
  
  nabl2-reduce-op-hook(|sol): ChoiceLeftMerge(TOne()       , _            ) -> TOneOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMerge(TOneOrMore() , _            ) -> TOneOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMerge(_            , TOne()       ) -> TOneOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMerge(_            , TOneOrMore() ) -> TOneOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMerge(TZeroOrOne() , TZeroOrOne() ) -> TZeroOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMerge(TZeroOrMore(), TZeroOrOne() ) -> TZeroOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMerge(TZeroOrOne() , TZeroOrMore()) -> TZeroOrMore()
  nabl2-reduce-op-hook(|sol): ChoiceLeftMerge(TZeroOrMore(), TZeroOrMore()) -> TZeroOrMore()
  