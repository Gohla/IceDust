module namedentities

model
entity Node{
	value : Int
	rootVal : Int = root . value
	childVal : Int = avg( root > parent]Dominates[child .value ) <+ 0
}
relation Dominates {
	Node + parent
	Node 1 child
	value : Int =  this > Dominates[parent . value
}

data
Node n1{
	value = 2
}
Node n2{
	value = 4
}
Node root{
	value = 3
}

Dominates t1{
	parent:root
	child:n1
}
Dominates t2{
	parent:root
	child:n2
} 

execute
n1.value
n2.value
root.value 