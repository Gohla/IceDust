module generate-expressions-navigators-multiplicities

language Relations

setup Common [[
module ExpressionsNavigatorsMultiplicities

model
entity Node{
	a : Int  = 1
}
relation Tree{
	Node * parent
	Node ? child
}

data
Node n1{}
Node n2{}
Tree{
	parent: n1
	child : n2
}

execute
]]

test Expressions Navigators Multiplicities generate-java [[ ]] build generate-java-test

test Expressions Navigators Multiplicities compile-java [[ ]] build compile-java-test

test Expressions Navigators Multiplicities execute-java 1 in single [[ n2 > child]Tree[parent . a ]] build execute-java-test to "1"
