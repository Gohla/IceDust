module names-members

language Relations

setup Common [[
	module members

	model

]]

test (Wontfix member namespaces) error on attribute and role with same name [[
	entity Student{	}
	
	relation Enrollment{
		Student * [[student]] <- enrollment
		[[student]] : Int
	}
]] 2 errors

test (Wontfix member namespaces) error on attribute and inverse with same name [[
	entity Student{
		[[enrollment]] : Int
	}
	
	relation Enrollment{
		Student * student <- [[enrollment]]
	}
]] 2 errors

test (Wontfix member namespaces) error on attribute and shortcut with same name [[
	entity Student{
		[[course]] : Int
	}
	
	entity Course{
		
	}
	
	relation Enrollment{
		Student * student <- enrollment
		Course *  course  <- enrollment
		
		student.[[course]] <-> course.student
	}
]] 2 errors

test (Wontfix member namespaces) error on role and inverse with same name [[
	relation Student{
		Enrollment * [[enrollment]] <- x
	}
	
	relation Enrollment{
		Student * student <- [[enrollment]]
	}
]] 2 errors

