module types-data

language Relations

setup Common [[
	module test
]]


test error on omitting required attribute [[
	model
	entity X{
		i : Int
	}
	data
	X [[x]]{}
]] 1 error

test error on omitting required attribute multiple [[
	model
	entity X{
		i : Int
		j : String
	}
	data
	X [[x]]{}
]] 1 errors

test no error on omitting optional attribute[[
	model
	entity X{
		i : Int ?
	}
	data
	X x{}
]] 0 errors

test error on omitting only on required attribute[[
	model
	entity X{
		i : Int ?
		j : String
	}
	data
	X [[x]]{}
]] 1 error

