module books

model

	entity Book{
		title : String
	}
	
	entity Reader{
		name : String
	}
	
	entity Lender{
		numSubscribers : Int = count(subscriber)
		maxSubscribers : Int
		maxSubscribersReached : Boolean = numSubscribers >= maxSubscribers
	}
	
	relation Own{
		Lender * -> owner 
		Book 1   -> holding
	}
	
	relation Borrow{
		Reader * -> borrower
		Book ?   -> borrowed
	}
	relation Subscribe{
		Reader * -> subscriber
		Lender * 
	}
