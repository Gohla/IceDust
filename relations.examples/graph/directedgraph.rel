module directedgraph

model

	entity Node {
	}
	
	relation Edge {
	  Node * from <- asFrom
	  Node * to   <- asTo
	}

data

	Node a {
	}
	
	Node b {
	}
	
	Node c {
	}
	
	Edge ab {
	  from : a
	  to : b
	}
	
	Edge bc {
	  from : b
	  to : c
	}
	
	Edge ca {
	  from : c
	  to : a
	}

execute

	a
	b
	c
	a.to //must be b
	a.from //must be c
	a.to.to //must be c
	a.to.to.to //must be a
