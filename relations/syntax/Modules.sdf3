module Modules

imports
	Common
	Model
	Rules
	Data
	Execute


templates // modules

	Start.Module = <
		module <ID>
		
		<ModelDef?>

		<RulesDef?>
		
		<DataDef?>
		
		<Executable?>
		
	>

	ModelDef.ModelDef = <
		model
		
		  <Model*;separator="\n\n">
	>

	RulesDef.RulesDef = <
		rules
		
		  <Rule*;separator="\n\n">
	>
	
	DataDef.DataDef = <
		data
		
		  <Data*;separator="\n\n">
	>
	
	Executable.Executable = <
		execute
		
		  <Execute*;separator="\n\n">
  >
