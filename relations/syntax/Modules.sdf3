module Modules

imports
	Common
	Model
	Data
	Execute


context-free syntax // modules

	Start.Module = <
		module <ID>
		
		<ModelDef?>
		
		<DataDef?>
		
		<Executable?>
		
	>

	ModelDef.ModelDef = <
		model
		
		  <{Model "\n\n"}*>

	>
	
	DataDef.DataDef = <
		data
		
			<{Data "\n\n"}*>
	>
	
	Executable.Executable = <
		execute
		
			<{Execute "\n\n"}*>
  >
