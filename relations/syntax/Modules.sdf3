module Modules

imports
	Common
	Model
	Rules
	Data
	Execute


context-free syntax // modules

	Start.Module = <
		module <ID>
		
		<ModelDef?>
		
		<RulesDef?>
		
		<DataDef?>
		
		<Executable?>
		
	>

	ModelDef.ModelDef = <
		model
		
		  <{Model "\n\n"}*>
		
	>

	RulesDef.RulesDef = <
		rules
		
			<{Rule "\n\n"}*>
	>
	
	DataDef.DataDef = <
		data
		
			<{Data "\n\n"}*>
	>
	
	Executable.Executable = <
		execute
		
			<{Execute "\n\n"}*>
  >
