module trans/generating/expressions

imports

	include/Relations
	lib/java/Java

rules

	to-java: Addition(exp1, exp2)				-> Plus(	<to-java>exp1, <to-java>exp2)
	to-java: Subtraction(exp1, exp2)		-> Minus(	<to-java>exp1, <to-java>exp2)
	to-java: Division(exp1, exp2) 			-> Div(		<to-java>exp1, <to-java>exp2)
	to-java: Modulo(exp1, exp2) 				-> Remain(<to-java>exp1, <to-java>exp2)
	to-java: Multiplication(exp1, exp2)	-> Mul(		<to-java>exp1, <to-java>exp2)
