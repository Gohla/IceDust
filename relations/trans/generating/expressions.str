module trans/generating/expressions

imports

	lib/java/Java
	trans/desugaring/constructors

rules

	to-java: BinExp(exp1, Plus(), exp2)		-> Plus(	<to-java>exp1, <to-java>exp2)
	to-java: BinExp(exp1, Minus(), exp2)	-> Minus(	<to-java>exp1, <to-java>exp2)
	to-java: BinExp(exp1, Div(), exp2)		-> Div(		<to-java>exp1, <to-java>exp2)
	to-java: BinExp(exp1, Mod(), exp2)		-> Remain(<to-java>exp1, <to-java>exp2)
	to-java: BinExp(exp1, Mul(), exp2)		-> Mul(		<to-java>exp1, <to-java>exp2)
