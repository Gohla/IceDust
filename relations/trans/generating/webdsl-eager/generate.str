module generating/webdsl-eager/generate

imports // constructors

	signatures/Modules-sig

imports // functions

	webdsl/pp-webdsl
	generating/webdsl-eager/module
	webdsl/WebDSL-parenthesize

rules // Builders
	
	generate-webdsl-eager-code(err):
	    (selected, path) -> (fileName, result, errors)
	    with
			fileName     := <guarantee-extension(|"app")> path;
			webdsl-ast   := <to-webdsl-eager(err)> selected;
			errors       := <collect-om(\("ERROR_PLACEHOLDER", msg) -> msg\)>;
			webdsl-ast'  := <bottomup(try(\("ERROR_PLACEHOLDER", msg) -> []\))> webdsl-ast;
			webdsl-ast'' := <parenthesize-WebDSL>webdsl-ast';
			result       := <pp-webdsl-to-string> webdsl-ast''

rules	// General Java generation rules

	to-webdsl-eager(err) = where(is-string);id

  to-webdsl-eager(err):
    [_] -> <concat-strings> <map(to-webdsl-eager(err))>
    
  to-webdsl-eager(err):
    () -> ""
    
  to-webdsl-eager(err):
  	None() -> ""

  to-webdsl-eager(err): Module(_,_,_,_) -> None()
  	where
  		<err> "Desugar before generating code."
	
  to-webdsl-eager(err): a -> a'
  	where
		  a' := <err>$[No to-webdsl-eager defined for [<get-constructor> a]/[<get-arguments;length> a].]

