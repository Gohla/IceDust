/**
* Example Relations program.
*/  
module example

entity User {
	name     : String
	password : String
	numPosts : Int
}

entity BlogPost {
	title : String
	body  : String
}

relation Posting {
	poster   : User
	coAuthor : User
	postee   : BlogPost
	numEdits : Int
	editMsg  : String
}

entity Log {
	
}

relation LogItem {
	log : Log
	change : Posting
}

from Log join T.A

from Log

from Log

