module example

entity User {
	name     : String
	password : String
	numPosts : Int
}

entity BlogPost {
	title : String
	body  : String
}

relation Posting {
	User * poster
	BlogPost 1 post
	
	numEdits : Int
	editMsg  : String
}

entity Log {
	
}

relation LogItem {
	Log 1
	Posting *
	Test ?
}

from Log join T.A

from Log

