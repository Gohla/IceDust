module example

model
entity BlogUser {
	name     : String
	password : String
	numPosts : Int
}
entity BlogPost {
	title : String
	body  : String
	name  : String
}
relation Posting {
	BlogUser * poster
	BlogPost 1 post
	numEdits : Int
	editMsg  : String
}
entity Log {
}
relation LogItem {
	Log 1
	Posting *
}

execute
new BlogUser{}{BlogUser.name="My name"}
new BlogPost{Posting=x}{BlogPost.title="First post" BlogPost.body="Dummy text"}
new BlogPost{Posting=x}{BlogPost.title="Second post" BlogPost.body="More dummy text"}
from BlogPost join Posting where BlogUser.name = "My name"