module example

model
entity BlogUser {
	name     : String
	password : String
	numPosts : Int
}
entity BlogPost {
	title : String
	body  : String
	name  : String
}
relation Posting {
	BlogUser * poster
	BlogPost 1 post
	numEdits : Int
	editMsg  : String
}
entity Log {
}
relation LogItem {
	Log 1
	Posting *
}

execute
BlogUser x = new BlogUser{}{name="Your name"}
BlogPost x = new BlogPost{Posting=x}{title="First post" body="Dummy text"}
BlogPost x = new BlogPost{Posting=x}{title="Second post" body="More dummy text"}
BlogPost* x = from BlogPost join Posting where BlogUser.name = "My name"

BlogPost* x = x > Posting > BlogPost
BlogPost* x = x >> Posting ->> BlogPost
Log* x = x >> Posting ->> LogItem -> Log