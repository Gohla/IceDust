/**
* Example Relations program.
*/  
module example

model
entity BlogUser {
	name     : String
	password : String
	numPosts : Int
}
entity BlogPost {
	title : String
	body  : String
	name  : String
}
relation Posting {
	BlogUser * poster
	BlogPost 1 post
	
	numEdits : Int
	editMsg  : String
}
entity Log {
	
}
relation LogItem {
	Log 1
	Posting *
}

execute
new BlogUser{}{name=x}
new BlogPost{Posting=x}{title=x body=x}
new BlogPost{Posting=x}{title=x body=x}
from BlogPost join Posting where BlogUser.name = x
from Log



