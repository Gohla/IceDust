module example

model
entity User {
	name  : String = "no name"	(default)
	name2 : String = name				(derivation)
	name3 : String = name2
	age		: Int
}

relation Friendship {
	User 1 friend1
	User 1 friend2
}


data
User me{
	name="my name"
}
User you{
	name="your name"
}
Friendship us{
	friend1:me
	friend2:you
}

execute
me.name