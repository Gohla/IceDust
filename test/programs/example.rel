module example

model
entity User {
	name     : String
	password : String
	numPosts : Int
}
entity Post {
	title : String
	body  : String
	name  : String
}
relation Posting {
	User * poster
	Post 1 post
	numEdits : Int
	editMsg  : String
}
entity Log {
}
relation LogItem {
	Log 1
	Posting *
}

data
User me{
	name="My name"
	password="notEncoded"
	numPosts=1
}
Post first{
	title="First post"
	body="Some text"
	name="another name"
}
Post second{
	title="Second post"
	body="Some more text"
	name="yet another name"
	name=1						//TODO: should trigger a type error
}
Posting p1{
	poster:me
	post:first
	numEdits=1
}
Posting p2{
	poster:me
	post:second
}

execute
me.name
me.password
me.numPosts
me > Posting > Post
me ->> Posting -> LogItem >> Log