module example

model
entity BlogUser {
	name     : String
	password : String
	numPosts : Int
}
entity BlogPost {
	title : String
	body  : String
	name  : String
}
relation Posting {
	BlogUser * poster
	BlogPost 1 post
	numEdits : Int
	editMsg  : String
}
entity Log {
}
relation LogItem {
	Log 1
	Posting *
}

data
BlogUser me{
	name="My name"
	password="notEncoded"
	numPosts=1
}
BlogPost first{
	title="First post"
	body="Some text"
	name="another name"
}
BlogPost second{
	title="Second post"
	body="Some more text"
	name="yet another name"
}
Posting p1{
	poster:me
	post:first
	numEdits=1
}
Posting p2{
	poster:me
	post:second
}

execute
me > Posting > BlogPost
me ->> Posting -> LogItem >> Log