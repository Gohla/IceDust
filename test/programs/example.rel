/**
* Example Relations program.
*/  
module example

entity BlogUser {
	name     : String
	password : String
	numPosts : Int
}

entity BlogPost {
	title : String
	body  : String
	name  : String
}

relation Posting {
	BlogUser * poster
	BlogPost 1 post
	
	numEdits : Int
	editMsg  : String
}

entity Log {
	
}

relation LogItem {
	Log 1
	Posting *
}

from BlogPost join Posting where BlogUser.name = x

from Log



