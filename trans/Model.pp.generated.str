       ]

  prettyprint-Multiplicity =
    ?ZeroOrOne()
    ; ![ H(
           [SOpt(HS(), "0")]
         , [S("?")]
         )
       ]

  prettyprint-Multiplicity =
    ?OneOrMore()
    ; ![ H(
           [SOpt(HS(), "0")]
         , [S("+")]
         )
       ]

  prettyprint-Multiplicity =
    ?ZeroOrMore()
    ; ![ H(
           [SOpt(HS(), "0")]
         , [S("*")]
         )
       ]

  prettyprint-example =
    prettyprint-Attribute

  prettyprint-example =
    prettyprint-PrimitiveType

  prettyprint-example =
    prettyprint-Model

  prettyprint-example =
    prettyprint-RoleOrAttribute

