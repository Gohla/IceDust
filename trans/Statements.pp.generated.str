module Statements.pp.generated

strategies
  prettyprint-Block :
    Block(a) -> [ H(
                    [SOpt(HS(), "0")]
                  , [S("{")]
                  )
                , a298
                , H(
                    [SOpt(HS(), "0")]
                  , [S("}")]
                  )
                ]
    where a298 := <pp-indent(|"2")> [<pp-H-list(prettyprint-Statement)> a]

  prettyprint-Statement :
    a -> [ H(
             [SOpt(HS(), "0")]
           , [a299]
           )
         ]
    where a299 := <pp-one-Z(prettyprint-Block)> a

  prettyprint-Statement :
    Exp(a) -> [ H(
                  [SOpt(HS(), "0")]
                , [a300]
                )
              ]
    where a300 := <pp-one-Z(prettyprint-Expression)> a

  prettyprint-example =
    prettyprint-Block

  prettyprint-example =
    prettyprint-Statement